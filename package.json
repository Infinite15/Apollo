{
  "name": "apollo-client",
  "version": "0.0.1",
  "description": "Compile your Relay GraphQL queries at runtime, no need for Babel. Only for use in development.",
  "main": "./lib/index.js",
  "scripts": {
    "pretest": "npm run compile",
    "test": "mocha --reporter spec --full-trace test/index.js",
    "posttest": "npm run lint",
    "postinstall": "./node_modules/.bin/typings i",
    "compile": "tsc -p src",
    "prepublish": "npm run compile",
    "lint": "tslint src/*.ts && tslint test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "apollostack/apollo-client"
  },
  "keywords": [
    "ecmascript",
    "es2015",
    "jsnext",
    "javascript",
    "relay",
    "npm",
    "react"
  ],
  "author": "Sashko Stubailo <sashko@stubailo.com>",
  "license": "MIT",
  "dependencies": {
    "graphql": "^0.4.18",
    "lodash": "^4.6.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "dataloader": "^1.1.0",
    "lodash": "^4.5.1",
    "mocha": "^2.3.3",
    "request-promise": "^2.0.1",
    "tslint": "^3.6.0",
    "typescript": "^1.8.9",
    "typescript-require": "^0.2.9-1",
    "typings": "^0.7.9"
  }
}
